{
  "name": "ygency-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "postexport": "sh -c 'next-sitemap --config next-sitemap.config.js && cp -f public/favicon.ico out/ || true && cp -f public/site.webmanifest out/ || true && cp -f public/.htaccess out/ || true && node scripts/postexport-fix-en.js'",
