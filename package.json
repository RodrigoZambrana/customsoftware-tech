    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.10.1",
    "react": "18.2.0",
    "react-bootstrap": "^1.6.1",
    "react-circular-progressbar": "^2.1.0",
    "react-countup": "^6.4.2",
    "react-dom": "18.2.0",
    "react-i18next": "^15.6.1",
    "react-nice-select": "^1.0.4",
    "react-player": "^2.12.0",
    "react-slick": "^0.29.0",
    "react-visibility-sensor": "^5.1.1",
